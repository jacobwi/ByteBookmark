{
  "name": "bytebookmarks",
  "private": true,
  "version": "0.0.1",
  "description": "Bytebookmarks",
  "workspaces": [
    "apps/chrome-extension",
    "apps/web",
    "packages/shared"
  ],
  "scripts": {
    "web": "cd web && pnpm dev",
    "chrome-extension": "cd chrome-extension && yarn dev",
    "shared": "cd shared && pnpm dev",
    "lint": "eslint 'web/src/**/*.{js,jsx,ts,tsx}' 'shared/src/**/*.{js,jsx,ts,tsx}' 'chrome-extension/src/**/*.{js,jsx,ts,tsx}'",
    "pretest": "tsc jest.config.ts --outDir ./ --esModuleInterop",
    "test": "jest",
    "pretty": "prettier --write ."
  },
  "packageManager": "pnpm@8.15.5",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.70",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vitejs/plugin-react": "^4.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-svg-transformer": "^1.0.0",
    "jest-transformer-svg": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3",
    "vite": "^5.2.6"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 11",
    "defaults"
  ]
}
